{
  "name": "recipes",
  "version": "1.1.1",
  "description": "Application to manage recipes",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "clean": "npx rimraf dist",
    "build": "webpack --config webpack.config.js",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "lint": "eslint . --ext .jsx,.js webpack.config.*",
    "format": "prettier --check \"**/*.*\"",
    "lint:report": "eslint . --ext .jsx --ext .js src --output-file ./report/eslint-report.html --format html",
    "test": "jest --coverage",
    "test:watch": "yarn run test --watch",
    "pre-push": "npm run lint && npm run format && npm run test && npm run clean && npm run build"
  },
  "author": "nathan m weller",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21",
    "prop-types": "15.8.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.2.1",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "vanilla-toast": "0.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-react": "7.16.7",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.4",
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "12.1.4",
    "babel-loader": "8.2.3",
    "copy-webpack-plugin": "10.2.4",
    "css-loader": "6.7.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.11.0",
    "eslint-config-airbnb": "19.0.4",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.29.3",
    "file-loader": "6.2.0",
    "jest": "27.5.1",
    "prettier": "2.5.1",
    "react-refresh": "0.11.0",
    "redux-immutable-state-invariant": "2.1.0",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.70.0",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.7.4"
  }
}
