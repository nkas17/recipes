{
	"name": "recipes",
	"version": "1.1.1",
	"description": "Application to manage recipes",
	"scripts": {
		"start": "webpack-dev-server --config webpack.config.dev.js",
		"clean": "node node_modules/rimraf/bin.js dist/*",
		"build": "yarn run clean && webpack --config webpack.config.js",
		"analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
		"lint": "node_modules/.bin/esw webpack.config.* src --ext js --ext jsx --ignore-pattern assets --color",
		"lint:watch": "yarn run lint --watch",
		"test": "jest --coverage",
		"test:watch": "yarn run test --watch"
	},
	"author": "nathan m weller",
	"license": "MIT",
	"dependencies": {
		"lodash": "4.17.15",
		"prop-types": "15.7.2",
		"react": "16.12.0",
		"react-dom": "16.12.0",
		"react-hot-loader": "4.12.18",
		"react-redux": "7.1.3",
		"react-router": "5.1.2",
		"react-router-dom": "5.1.2",
		"redux": "4.0.4",
		"redux-thunk": "2.3.0",
		"vanilla-toast": "0.5.0"
	},
	"devDependencies": {
		"@babel/core": "7.7.4",
		"@babel/preset-env": "7.7.4",
		"@babel/preset-react": "7.7.4",
		"babel-loader": "8.0.6",
		"copy-webpack-plugin": "5.0.5",
		"css-loader": "3.2.0",
		"enzyme": "3.10.0",
		"enzyme-adapter-react-16": "1.15.1",
		"enzyme-to-json": "3.4.3",
		"eslint": "6.7.0",
		"eslint-config-airbnb": "18.0.1",
		"eslint-config-prettier": "6.7 I .0",
		"eslint-plugin-import": "2.18.2",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-prettier": "3.1.1",
		"eslint-plugin-react": "7.16.0",
		"eslint-watch": "6.0.1",
		"extract-text-webpack-plugin": "3.0.2",
		"file-loader": "4.3.0",
		"jest": "24.9.0",
		"prettier": "1.19.1",
		"redux-immutable-state-invariant": "2.1.0",
		"rimraf": "3.0.0",
		"style-loader": "1.0.0",
		"url-loader": "2.3.0",
		"webpack": "4.41.2",
		"webpack-bundle-analyzer": "3.6.0",
		"webpack-cli": "3.3.10",
		"webpack-dev-server": "3.9.0"
	}
}
