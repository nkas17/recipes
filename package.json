{
  "name": "recipes",
  "version": "1.1.1",
  "description": "Application to manage recipes",
  "scripts": {
    "start": "webpack-dev-server --config webpack.config.dev.js",
    "clean": "npx rimraf dist",
    "build": "webpack --config webpack.config.js",
    "analyze": "webpack --profile --json > stats.json && webpack-bundle-analyzer ./stats.json",
    "lint": "eslint . --ext .jsx,.js webpack.config.*",
    "lint:watch": "yarn run lint --watch",
    "format": "prettier --check \"**/*.*\"",
    "lint:report": "eslint . --ext .jsx --ext .js src --output-file ./report/eslint-report.html --format html",
    "test": "jest --coverage",
    "test:watch": "yarn run test --watch",
    "pre-push": "yarn lint && yarn format && yarn test && yarn clean && yarn build"
  },
  "author": "nathan m weller",
  "license": "MIT",
  "dependencies": {
    "lodash": "4.17.21",
    "prop-types": "15.7.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-redux": "7.2.6",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "redux": "4.1.2",
    "redux-thunk": "2.4.1",
    "vanilla-toast": "0.5.0"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/preset-env": "7.16.4",
    "@babel/preset-react": "7.16.0",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.2",
    "@testing-library/jest-dom": "5.15.1",
    "@testing-library/react": "12.1.2",
    "babel-loader": "8.2.3",
    "copy-webpack-plugin": "10.0.0",
    "css-loader": "6.5.1",
    "enzyme": "3.11.0",
    "enzyme-adapter-react-16": "1.15.6",
    "enzyme-to-json": "3.6.2",
    "eslint": "8.3.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-react": "7.27.1",
    "file-loader": "6.2.0",
    "jest": "27.3.1",
    "prettier": "2.5.0",
    "react-refresh": "0.11.0",
    "redux-immutable-state-invariant": "2.1.0",
    "rimraf": "3.0.2",
    "style-loader": "3.3.1",
    "url-loader": "4.1.1",
    "webpack": "5.64.4",
    "webpack-bundle-analyzer": "4.5.0",
    "webpack-cli": "4.9.1",
    "webpack-dev-server": "4.6.0"
  }
}
